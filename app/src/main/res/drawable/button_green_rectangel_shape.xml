<?xml version="1.0" encoding="utf-8"?>
<selector  xmlns:android="http://schemas.android.com/apk/res/android" android:enterFadeDuration="100" android:exitFadeDuration="100">

<!-- <bitmap android:src="@drawable/schedual_cl" /> -->

<item  android:state_pressed="true">
    <shape
        android:shape="rectangle">
        <solid android:color="@color/color_green_dark"></solid>
        <corners android:radius="45dp"/>
    </shape>

</item>
<!--///////////////-->
<item android:state_enabled="false">
    <shape
        android:shape="rectangle">
        <solid android:color="@color/color_green_light"></solid>
        <corners android:radius="45dp"/>
    </shape>
</item>
<!--//////////////-->
<item>
    <shape
        android:shape="rectangle">
        <solid android:color="@color/color_green_light"></solid>
        <corners android:radius="45dp"/>

    </shape>

</item>

</selector>





