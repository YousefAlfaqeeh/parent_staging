<resources   xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingPrefix">


    <style name="CheckBoxStyle" parent="Base.Widget.AppCompat.CompoundButton.CheckBox" >
        <item name="android:layout_gravity">start|center_vertical</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:button">@null</item>
        <item name="android:drawableRight">?android:attr/listChoiceIndicatorMultiple</item>
        <!--<item name="fontPath">@string/font</item>-->
        <item name="android:fontFamily">@font/roboto</item>
        <item name="fontFamily">@font/roboto</item>
    </style>


    <style name="SwitchStyle">
        <!--<item name="fontPath">@string/font</item>-->
        <item name="android:fontFamily">@font/roboto</item>
        <item name="fontFamily">@font/roboto</item>
        <item name="android:layout_margin">5dp</item>
        <!--<item name="android:scaleX">@integer/scale_ar_en</item>-->
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:layout_gravity">start|center_vertical</item>
        <!--<item name="android:textScaleX">@integer/scale_ar_en</item>-->
        <item name="android:minHeight">50dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="RadioButtonStyle" parent="Base.Widget.AppCompat.CompoundButton.RadioButton" >
        <!--<item name="android:fontFamily">@font/custom_font</item>-->
        <item name="android:button">@null</item>
        <item name="android:drawableRight">@android:drawable/btn_radio</item>
        <item name="android:layout_gravity">start|center_vertical</item>
        <item name="android:gravity">start|center_vertical</item>
    </style>

</resources>


